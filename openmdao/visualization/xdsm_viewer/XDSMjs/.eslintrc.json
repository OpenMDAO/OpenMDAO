{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {},
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "global-require": "off",
        "react/forbid-prop-types": "off",
        "no-restricted-syntax": "off",
        "linebreak-style": "off",
        "max-classes-per-file": "off",
        "import/no-unresolved": "off",
        "react/no-this-in-sfc": "off"
    }
}